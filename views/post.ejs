<!DOCTYPE html>
<html>
<head>
    <title>Markdown to HTML</title>
    <link rel="stylesheet" type="text/css" href="/css/style.css">
  </head>
<body>
    <a href="/">BACK Button</a>
    <h1> <%- title %></h1>
    <ul>
        <% keywords.forEach((keyword)=> { %>
            <li>
                <h2>
                    <%= keyword %>
                </h2>
               
            </li>
            <hr />
        <% }) %>
    </ul>

    <div>
        <%- content %>
    </div>  
</body>
</html>
